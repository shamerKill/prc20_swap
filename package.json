{
  "name": "react_template",
  "version": "0.1.0",
  "description": "react template project",
  "scripts": {
    "start": "rimraf ./dist && parcel serve --no-cache",
    "build": "rimraf ./dist && parcel build  --no-cache --no-optimize --no-source-maps",
    "parcel": "parcel"
  },
  "repository": "https://github.com/shamerKill/react-template.git",
  "author": "shamer",
  "license": "MIT",
  "private": false,
  "source": "./src/index.html",
  "targets": {
    "default": {
      "distDir": "./dist",
      "context": "browser",
      "sourceMap": true,
      "engines": {
        "browsers": "> 0.5%, last 2 versions, not dead"
      }
    }
  },
  "alias": {
    "$*": "./src/main/$1"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "2.5.0",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.0",
    "@types/react-transition-group": "^4.4.4",
    "parcel": "^2.5.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-router-dom": "^6.3.0",
    "react-transition-group": "^4.4.2"
  }
}
