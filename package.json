{
  "name": "react_template",
  "version": "0.1.0",
  "description": "react template project",
  "scripts": {
    "start": "rimraf ./dist && parcel serve --no-cache",
    "build": "rimraf ./dist && parcel build  --no-cache --no-optimize --no-source-maps",
    "parcel": "parcel"
  },
  "repository": "https://github.com/shamerKill/react-template.git",
  "author": "shamer",
  "license": "MIT",
  "private": false,
  "source": "./src/index.html",
  "targets": {
    "default": {
      "distDir": "./dist",
      "context": "browser",
      "sourceMap": true,
      "engines": {
        "browsers": "> 0.5%, last 2 versions, not dead"
      }
    }
  },
  "alias": {
    "$*": "./src/main/$1"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "2.5.0",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.0",
    "@types/react-transition-group": "^4.4.4",
    "assert": "^2.0.0",
    "buffer": "^6.0.3",
    "events": "^3.3.0",
    "https-browserify": "^1.0.0",
    "parcel": "^2.5.0",
    "querystring-es3": "^0.2.1",
    "rimraf": "^3.0.2",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "typescript": "^4.6.3",
    "url": "^0.11.0"
  },
  "dependencies": {
    "@types/echarts": "^4.9.16",
    "classnames": "^2.3.1",
    "cosmo-wallet-tool": "0.0.32",
    "dayjs": "^1.11.4",
    "echarts": "^5.3.3",
    "i18next": "^21.8.13",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-i18next": "^11.18.0",
    "react-router-dom": "^6.3.0",
    "react-toastify": "^9.0.8",
    "react-transition-group": "^4.4.2",
    "rxjs": "^7.5.5",
    "web3": "^1.7.5"
  }
}
